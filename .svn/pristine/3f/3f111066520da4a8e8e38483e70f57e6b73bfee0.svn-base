    LEFT = 'left',
    INNER = 'inner',
    OTHER = 'other',
}
export interface DragTransferDataInterface {
    theadType: string;
    target: TheadContainer;
    targetBearing: ContainerBearing;
    source: DragStartData;
}

export class DragTsData implements DragTransferDataInterface {
