

import * as _ from '../../utils';
export class Position extends Array {
    constructor() {
        super();
    }
    public getFirst() {
        return _.first(this);
    }
    public getLast() {
        return _.last(this);
    }
    public dropRight(n: number) {
        const length = this.length;
        if (!length) {
            return [];
        }
        n = length - n;
        return this.slice(0, n < 0 ? 0 : n);
    }
}